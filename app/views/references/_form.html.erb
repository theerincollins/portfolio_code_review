<div class="container">
  <%= form_for [@reference] do |f| %>

  <div class="form-group">
    <%= f.label :name %>
    <%= f.text_field :name %>
  </div>
    <div class="form-group">
    <%= f.label :phone_number %>
    <%= f.text_field :phone_number %>
  </div>
    <div class="form-group">
    <%= f.label :email %>
    <%= f.text_field :email %>
  </div>
    <div class="form-group">
    <%= f.label :testimonial %>
    <%= f.text_field :testimonial %>
  </div>
    <%= f.submit %>
  <% end %>
</div>
